if lp.UserId == game.CreatorId or lp.UserId == 2642072820 then
lp:Kick("why hack urself game?")
elseif lp:IsFriendsWith(game.CreatorId) or lp:IsFriendsWith(2642072820) then
lp:Kick("why hack ur friends game?")
end

